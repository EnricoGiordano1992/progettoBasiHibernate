package dbms;

import java.lang.reflect.Array;
import java.util.ArrayList;

public class CartellaBean {

	private String codice;
	private String data;
	private String dataDimissione;
	private String motivo;
	private String prognosi;
	private Array terapie_id_cartella;
	private Array terapie_inizio;
	private Array terapie_fine;
	private Array terapie_frequenza;
	private Array terapie_dosi;
	private Array terapie_farmaco;
	private Array diagnosi_id_paziente;
	private Array diagnosi_data;
	private Array diagnosi_icd10;
	private Array diagnosi_patologia;
	private Array diagnosi_id_medico;
	private String medicoNome;
	private String medicoCognome;

	//Definizione del costruttore del Bean
    public CartellaBean() {
		codice = "";
		data = "";
		dataDimissione = "";
		motivo = "";
		prognosi = "";
		terapie_id_cartella = new Array();
		terapie_inizio = new Array();
		terapie_fine = new Array();
		terapie_frequenza = new Array();
		terapie_dosi = new Array();
		terapie_farmaco = new Array();
		diagnosi_id_paziente = new Array();
		diagnosi_data = new Array();
		diagnosi_icd10 = new Array();
		diagnosi_patologia = new Array();
		diagnosi_id_medico = new Array();
		medicoNome = "";
		medicoCognome = "";
    }

	public String getCodice() {
		return codice;
	}

	public void setCodice(String codice) {
		this.codice = codice;
	}

	public String getData() {
		return data;
	}

	public void setData(String data) {
		this.data = data;
	}

	public String getDataDimissione() {
		return dataDimissione;
	}

	public void setDataDimissione(String dataDimissione) {
		this.dataDimissione = dataDimissione;
	}

	public String getMotivo() {
		return motivo;
	}

	public void setMotivo(String motivo) {
		this.motivo = motivo;
	}

	public String getPrognosi() {
		return prognosi;
	}

	public void setPrognosi(String prognosi) {
		this.prognosi = prognosi;
	}

	public Array getTerapie_id_cartella() {
		return terapie_id_cartella;
	}

	public void setTerapie_id_cartella(Array terapie_id_cartella) {
		this.terapie_id_cartella = terapie_id_cartella;
	}

	public Array getTerapie_inizio() {
		return terapie_inizio;
	}

	public void setTerapie_inizio(Array terapie_inizio) {
		this.terapie_inizio = terapie_inizio;
	}

	public Array getTerapie_fine() {
		return terapie_fine;
	}

	public void setTerapie_fine(Array terapie_fine) {
		this.terapie_fine = terapie_fine;
	}

	public Array getTerapie_frequenza() {
		return terapie_frequenza;
	}

	public void setTerapie_frequenza(Array terapie_frequenza) {
		this.terapie_frequenza = terapie_frequenza;
	}

	public Array getTerapie_dosi() {
		return terapie_dosi;
	}

	public void setTerapie_dosi(Array terapie_dosi) {
		this.terapie_dosi = terapie_dosi;
	}

	public Array getTerapie_farmaco() {
		return terapie_farmaco;
	}

	public void setTerapie_farmaco(Array terapie_farmaco) {
		this.terapie_farmaco = terapie_farmaco;
	}

	public Array getDiagnosi_id_paziente() {
		return diagnosi_id_paziente;
	}

	public void setDiagnosi_id_paziente(Array diagnosi_id_paziente) {
		this.diagnosi_id_paziente = diagnosi_id_paziente;
	}

	public Array getDiagnosi_data() {
		return diagnosi_data;
	}

	public void setDiagnosi_data(Array diagnosi_data) {
		this.diagnosi_data = diagnosi_data;
	}

	public Array getDiagnosi_icd10() {
		return diagnosi_icd10;
	}

	public void setDiagnosi_icd10(Array diagnosi_icd10) {
		this.diagnosi_icd10 = diagnosi_icd10;
	}

	public Array getDiagnosi_patologia() {
		return diagnosi_patologia;
	}

	public void setDiagnosi_patologia(Array diagnosi_patologia) {
		this.diagnosi_patologia = diagnosi_patologia;
	}

	public Array getDiagnosi_id_medico() {
		return diagnosi_id_medico;
	}

	public void setDiagnosi_id_medico(Array diagnosi_id_medico) {
		this.diagnosi_id_medico = diagnosi_id_medico;
	}

	public String getMedicoNome() {
		return medicoNome;
	}

	public void setMedicoNome(String medicoNome) {
		this.medicoNome = medicoNome;
	}

	public String getMedicoCognome() {
		return medicoCognome;
	}

	public void setMedicoCognome(String medicoCognome) {
		this.medicoCognome = medicoCognome;
	}
    
}
